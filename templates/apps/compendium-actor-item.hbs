<form>
    <div class="form-group">
        <label>Source pack</label>
        <div class="form-fields">
            <select name="flags.wire.compendiumSource">
                <option value=""></option>
                {{selectOptions availablePacks selected=item.flags.wire.compendiumSource}}
            </select>
        </div>
    </div>
    <div class="form-group">
        <label>Imported version</label>
        <div class="form-fields">
            <input type="text" name="flags.wire.compendiumVersion" value="{{item.flags.wire.compendiumVersion}}">
        </div>
    </div>
    <div class="form-group">
        <label>Source module version</label>
        <div class="form-fields">
            {{sourceModule.version}}
        </div>
    </div>
</form>
