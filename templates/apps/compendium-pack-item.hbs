<form>
    <div class="form-group">
        <label>Module version</label>
        <div class="form-fields">
            <div class="flexrow">
                <div class="flex1">{{module.version}}</div>
                <div class="flex0"><a action="use-module-version"><i class="fas fa-eye-dropper"></i></a></div>
            </div>
        </div>
    </div>
    <div class="form-group">
        <label>Item version</label>
        <div class="form-fields">
            <input type="text" name="flags.wire.compendiumVersion" value="{{item.flags.wire.compendiumVersion}}">
        </div>
    </div>
</form>
